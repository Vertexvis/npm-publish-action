    "glob": "^7.2.0",
    "regenerator-runtime": "^0.13.8"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@types/glob": "^7.1.1",
    "@types/jest": "^25.2.3",
    "@types/node": "^13.13.4",
    "@types/regenerator-runtime": "^0.13.1",
    "@vertexvis/eslint-config-vertexvis-typescript": "^0.4.0",
